{
  "presets": ["babel-preset-env", "babel-preset-react"],
  "env": {
    "production": {
      "plugins": [
        ["emotion", {
          "hoist": true
        }]
      ]
    },
    "development": {
      "plugins": [
        ["emotion", {
          "sourceMap": true,
          "autoLabel": true,
          "labelFormat": "⚡[local]"
        }]
      ]
    },
    "test": {
      "plugins": [
        ["emotion", {
          "sourceMap": true,
          "autoLabel": true,
          "labelFormat": "⚡[local]"
        }]
      ]
    }
  }
}